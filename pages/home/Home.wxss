.home-swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.home-swiper swiper image {
  width: 100%;
}
.home-swiper .home-cate {
  display: flex;
}
.home-swiper .home-cate navigator {
  flex: 1;
  padding: 20rpx;
}
.home-swiper .home-cate navigator image {
  width: 100%;
}
.home-swiper .home-floor .floor-group .floor-title image {
  width: 100%;
}
.home-swiper .home-floor .floor-group .floor-list {
  overflow: hidden;
  padding: 10rpx;
}
.home-swiper .home-floor .floor-group .floor-list navigator {
  float: left;
  width: 33.33%;
}
.home-swiper .home-floor .floor-group .floor-list navigator:nth-last-child(-n+4) {
  height: calc(33.33vw * 386 / 232 / 2 - 5rpx);
  border-left: 10rpx solid #fff;
}
.home-swiper .home-floor .floor-group .floor-list navigator:nth-child(2),
.home-swiper .home-floor .floor-group .floor-list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.home-swiper .home-floor .floor-group .floor-list navigator image {
  width: 100%;
  height: 100%;
}
